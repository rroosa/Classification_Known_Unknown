{
    "project": "known_classes",
    "description": "Training the model on dataset of known classes",
    "folder_photos": "Imagenet_photos",
    "known_name_classes": [
        "bookcase",
        "gorilla",
        "tiger",
        "dough"
    ],
    "src_dataset": "IMAGENET",
    "experiment_[2]": {
        "num_epochs": 10,
        "batch_size": 32,
        "balanced": true,
        "network": {
            "architecture": "ResNet18",
            "pretrained": true
        },
        "hyperparameters": {
            "lr": 0.01,
            "weight_decay": 0.005,
            "momentum": 0.9
        },
        "criterion": {
            "type": "CrossEntropyLoss"
        },
        "scheduler": {
            "type": "StepLR"
        }
    }
}